location ~ ^/sg/(concat|login|logout) {
    allow all;
    proxy_pass $epi_ui_us;
    break;
}

# CAC Fonts
location ~ ^\/ayuda\/.*(eot|otf|ttf|woff)$ {
    proxy_pass $cac_frontend_us;
}

location ~* \.(eot|otf|ttf|woff)$ {
    proxy_pass $epi_ui_us;
}

location ~ ^/carrito/compra/(pagada|rechazada) {
	proxy_pass $epi_ui_us;
}
